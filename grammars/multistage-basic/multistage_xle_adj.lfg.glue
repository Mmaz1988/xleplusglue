GLUE   BASIC   TEMPLATES (1.0)

   LLIMP(N L R) = (N ANT) = L
	  (N CONS) = R.
	      
   LLATOM(N L T) = (N RESOURCE) = L
	  (N TYPE) = T.

   LLCLOSURE(G) = G $ (g::* GLUE).

   GLUE-MEANING(TypedRES M) = 
	  (TypedRES MEANING) = M.

   NOSCOPE(G) = (G NOSCOPE) = +.  

   QUANT-SCOPE(UP NAME) =
	  NAME = s::({SUBJ | OBJ | OBJ2 | COMP | OBL-TO | OBL-AG | OBL}+ ^).

   PREP-SEM(P) =
             {
         ((ADJUNCT ^) TNS-ASP)
	     %scope = s::(ADJUNCT ^)
	     %ppscope = (ADJUNCT ^)
	    :$ [/U_<e,t>.[/V_<e,t>.[/x_e.(U(x) & V(x))]]] :
	    ((s::(%ppscope SUBJ)_e -o s::^_t) -o ((s::(%ppscope SUBJ)_e -o s::%ppscope_t) -o (s::(%ppscope SUBJ)_e -o s::%ppscope_t))) || noscope,
	    :$ [/x_e.[/y_e.P(x,y)]] : (s::(%ppscope SUBJ)_e -o (s::(^OBJ)_e -o s::^_t)),
	    |
	    ((ADJUNCT ^) NTYPE)
	    %scope = s::(ADJUNCT ^)
	    %ppscope = ({SUBJ | OBJ | OBJ2 | COMP | OBL-TO | OBL-AG | OBL}+ ADJUNCT ^)
	    :$ [/U_<e,t>.[/V_<e,t>.[/x_e.(U(x) & V(x))]]] :
	    ((%scope_e -o s::^_t) -o ((s::(%ppscope OBJ)_e -o s::%ppscope_t) -o (s::(%ppscope OBJ)_e -o s::%ppscope_t))) || noscope,
	    :$ [/x_e.[/y_e.P(x,y)]] : (%scope_e -o (s::(^OBJ)_e -o s::^_t)),
	     }.

